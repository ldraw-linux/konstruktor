<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/x-ldraw">
    <sub-class-of type="text/plain"/>
    <comment>LDraw document</comment>
    <glob weight="10" pattern="*.dat"/>
    <glob weight="50" pattern="*.ldr"/>
  </mime-type>
  <mime-type type="application/x-multi-part-ldraw">
    <sub-class-of type="text/plain"/>
    <comment>Multipart LDraw document</comment>
    <glob pattern="*.mpd"/>
  </mime-type>
</mime-info>

